{
    "settings": {
        "ecmascript": 6,
        "jsx": false
    },
    "env": {
        "browser": true
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "binaryLiterals": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "modules": true,
        "templateStrings": true,
        "unicodeCodePointEscapes": true
    },
    "rules": {
        "quotes": [1, "single", "avoid-escape"],
        "strict": [1, "function"],
        "global-strict": [0],
        "no-extra-strict": [0],
        "no-shadow": [0]
    }
}